{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "react"
  ],
//  "settings": {
//    "import/resolver": {
//      "node": {
//        "paths": ["src"],
//        "extensions": [".js", ".jsx", ".ts", ".tsx"]
//      }
//    }
//  },
  "rules": {
    "react-hooks/rules-of-hooks": "off",
//    "self-closing-html-only": "error",     "./eslint/rules/self-closing-html-only.js" todo
    "react/no-unescaped-entities": 0,
    "react/jsx-key": "error",
    "react/prop-types": "off",
    "react/jsx-no-duplicate-props": "error",
    "react/no-unknown-property": ["error", { "ignore": ["jsx", "global"] }],
    "react/jsx-no-useless-fragment": "error",
//    "react/self-closing-comp": "error",
//    "react/jsx-props-no-spreading": ["warn", {
//      "html": "ignore",
//      "custom": "enforce"
//    }],

//    "no-unused-vars": "warn",
    "eqeqeq": ["error", "always"],
    "complexity": ["warn", { "max": 10 }],
//    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "@typescript-eslint/no-unused-vars": ["off"], // until best times
    "@typescript-eslint/ban-ts-comment": ["off"],
//    "@typescript-eslint/ban-ts-comment": ["warn", { "ts-ignore": "allow-with-description" }],
    "@typescript-eslint/no-explicit-any": "off",
//    "@typescript-eslint/no-implicit-any": "error" todo google later or add to ts config
    "@typescript-eslint/no-empty-object-type": "off"

  },
  "overrides": [
    {
      "files": ["src/shared/**/*.{ts,tsx}"],
      "rules": {
        "no-unused-vars": 0,
        "@typescript-eslint/no-unused-vars": 0
      }
    }
  ]
}
